<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">



    <parameters>
        <parameter key="wodor_net_moto_trip.datatable_paginator.class">WodorNet\MotoTripBundle\DataTable\Paginator</parameter>
    </parameters>

    <services>
        <service id="wodor_net_moto_trip.datatable_paginator" class="%wodor_net_moto_trip.datatable_paginator.class%" scope="request">
            <argument type="service" id="knp_paginator" />
            <argument type="service" id="request" />
            <argument type="service" id="templating.engine.twig" />
        </service>

        <service id="wodor_net_moto_trip.type.mapPoint" class="WodorNet\MotoTripBundle\Form\MapPointType" scope="prototype">
            <argument type="service" id="ivory_google_map.map" />
            <argument type="service" id="ivory_google_map.event" />
            <argument type="service" id="ivory_google_map.marker" />
            <tag name="form.type" alias="mapPoint" />
        </service>

    </services>

    
</container>
